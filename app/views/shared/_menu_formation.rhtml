		<% end %>
	</ul>
	</dd>
	<dt style="list-style-type: none;"><%= link_to "Saint Claude", enseignements_path(:tag => @saint_claude) %></dt>
		<dt style="list-style-type: none;"><%= link_to "Schoelcher", enseignements_path(:tag => @schoelcher) %></dt>
