		<ul>
			<li style="list-style-type: none;">
			<%= link_to "Présentation".t, :controller => "pages", :action => "master" , :anchor => "presentation" %>
			</li>
			<li style="list-style-type: none;"><%= link_to "M1".t, enseignements_path(:tag => @m1) %></li>
			<li style="list-style-type: none;"><%= link_to "M2".t, enseignements_path(:tag => @m2) %></li>
			<li style="list-style-type: none;"><%= link_to "Tous".t, enseignements_path(:tag => @master) %></li>
		</ul>
