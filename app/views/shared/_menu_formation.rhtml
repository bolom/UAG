<dl>
	<dt><%= link_to "Licence".t, :controller => "pages", :action => "formations" %></dt>
	<dd>
		<ul>
			<li style="list-style-type: none;"><%= link_to "Présentation".t, :controller => "pages", :action => "formations" , :anchor => "presentation" %></li>
					<li style="list-style-type: none;"><%= link_to "L1".t, enseignements_path(:tag => @l1) %></li>
					<li style="list-style-type: none;"><%= link_to "L2".t, enseignements_path(:tag => @l2) %></li>
					<li style="list-style-type: none;"><%= link_to "L3".t, enseignements_path(:tag => @l3) %></li>
			<li style="list-style-type: none;">
			<%= link_to "Tous".t, enseignements_path(:tag => @licence) %></li>
		</ul>
	</dd>
	<dt><%= link_to "Master &amp; Doctorat".t, :controller => "pages", :action => "master" %></dt>
